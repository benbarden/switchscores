{% extends 'theme/wos/staff/clean-wide.twig' %}

{% block page_inner %}

{% if GameList.count > 0 %}
    <div class="row">
        <div class="col-xs-9 col-sm-9 col-md-9">
            <h2>With release date</h2>
            {% include 'games/layouts/gridStaffEshopOrder/layout.twig' with {'GameList': GameList, 'batchOverride': 2} %}
        </div>
        <div class="col-xs-3 col-sm-3 col-md-3">
            <h2>No release date</h2>
            {% include 'games/layouts/gridStaffEshopOrder/layout.twig' with {'GameList': GameListNoDate, 'batchOverride': 1} %}
        </div>
    </div>
{% endif %}

{% endblock page_inner %}

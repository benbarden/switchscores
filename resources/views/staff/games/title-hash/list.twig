{% extends 'theme/staff-b5/layout-default.twig' %}

{% block page_inner %}

{% if TitleHashList.count > 0 %}
    {% include 'staff/games/title-hash/table.twig' with {'TitleHashList': TitleHashList} %}
{% else %}
    <div class="row">
        <div class="col-12 col-sm-3">
            <a href="{{ route('staff.games.title-hash.add') }}" class="btn outline btn-primary">Add</a>
        </div>
    </div>
    <hr>
    <p>No records found!</p>
{% endif %}

{% include 'ui/layouts/assets/table-sorting-b5.twig' %}
<script>
    $(document).ready(function() {
        $('.data-sortable').DataTable({
            "order": [{{ jsInitialSort|raw }}],
            "pageLength": 25,
            "columns": [
                null,
                null,
                null,
                null,
                { "orderable": false }
            ]
        });
    });
</script>

{% endblock page_inner %}

{% if errors.any() %}
    <div class="alert alert-danger">
        <p><strong>The following errors occurred:</strong></p>
        <ul>
            {% for error in errors.all() %}
                <li>{{ error }}</li>
            {% endfor %}
        </ul>
    </div>
{% endif %}

{% if ErrorsCustom %}
    <div class="alert alert-danger">
        <p><strong>The following errors occurred:</strong></p>
        <ul>
            {% for error in ErrorsCustom %}
                <li>{{ error }}</li>
            {% endfor %}
        </ul>
    </div>
{% endif %}
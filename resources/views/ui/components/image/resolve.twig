{% macro header(game) %}
    {% if game.image_header %}
        {{ '/img/ps-header/' ~ game.image_header }}
    {% else %}
        ''
    {% endif %}
{% endmacro %}

{% macro square(game) %}
    {% if game.image_square %}
        {{ '/img/ps-square/' ~ game.image_square }}
    {% else %}
        ''
    {% endif %}
{% endmacro %}

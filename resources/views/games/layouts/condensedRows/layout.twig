{% if GameList|length %}
    {% for item in GameList %}
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12">
                {% include 'games/layouts/condensedRows/item.twig' with {'item': item} %}
            </div>
        </div>
    {% endfor %}
{% else %}
    <p>No games found!</p>
{% endif %}

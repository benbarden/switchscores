<h2>Nintendo Switch games released in {{ Year }}</h2>
<div class="row">
    <div class="col-sm-6 col-md-6 col-lg-6">
        <table class="table table-responsive table-condensed">
            <tr>
                <th>Date</th>
                <th class="text-center">Games released</th>
                <th></th>
            </tr>
            {% for item in DateList %}
                <tr>
                    <td>
                        <a href="{{ route('games.browse.byDate.page', {'date': item.DateRaw}) }}">
                            {{ item.DateRaw|date('M Y') }}
                        </a>
                    </td>
                    <td class="text-center">
                        {{ item.GameCount }}
                    </td>
                    <td>
                        <a href="{{ route('games.browse.byDate.page', {'date': item.DateRaw}) }}">
                            View games
                        </a>
                    </td>
                </tr>
            {% endfor %}
        </table>
    </div>
    <div class="col-sm-6 col-md-6 col-lg-6">
        <h2>Switch releases by month</h2>
        {% include 'games/browse/byDateLandingChart.twig' with {'ChartDataSet': DateList} %}
    </div>
</div>

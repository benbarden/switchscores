<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for item in IndexList %}
        <sitemap>
            <loc>{{ url(item.XmlFile) }}</loc>
            <lastmod>{{ item.Timestamp }}</lastmod>
        </sitemap>
    {% endfor %}
</sitemapindex>
